package capture;

struct position_t {
    int64_t utime;
    // If projection is the empty string, easting and northing will be assumed
    // to be longitude and latitude (respectively) in WGS84 CRS.
    string  projection;
    double  easting;  // meters, unless longitude (then dec. degrees)
    double  northing; // meters, unless longitude (then dec. degrees)
    float   height;   // meters, UP is positive
}

